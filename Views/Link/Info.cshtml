<div class="container center-align">
<div class="row">
    <div class="col s12">
        
        @foreach (var item in ViewBag.Info)
        {
            <h5 class="dh right-align" style="direction: rtl">@item.Title</h5>
        <hr>
            <img src="~/LinkPic/@item.ImgUrl" class="responsive-img" style="width: 50%">
             <p class="dh" style="direction: rtl;text-align: justify;">
            @item.Text
        </p>
        }
       
    </div>
</div>
</div>